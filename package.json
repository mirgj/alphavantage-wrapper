{
  "name": "alphavantage-wrapper",
  "version": "0.5.0",
  "description": "Wrapper around alphavantage API",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/*",
    "lint:fix": "eslint src/* --fix",
    "build": "rm -rf lib && ./node_modules/.bin/babel src --out-dir lib --copy-files && del lib/**/*.spec.js lib/*.spec.js",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirgj/alphavantage-wrapper.git"
  },
  "keywords": [
    "alphavantage",
    "API",
    "wrapper",
    "stocks",
    "crypto",
    "market",
    "intraday",
    "forex",
    "sector",
    "performance"
  ],
  "author": "mirgj <mirgj@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mirgj/alphavantage-wrapper/issues"
  },
  "homepage": "https://github.com/mirgj/alphavantage-wrapper#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "core-js": "^3.1.4",
    "del-cli": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
